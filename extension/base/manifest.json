{
    "name": "Compass",
    "version": "1.0.0",
    "manifest_version": 2

    "default_locale": "en",
    "description": "Minimal and Elegant New Tab Page. Brought to you by DotHQ.",
    "icons": {
        "16": "images/Compass_Logo.png",
        "32": "images/Compass_Logo.png",
        "48": "images/Compass_Logo.png",
        "128": "images/Compass_Logo.png"
    },

    "permissions": [
        "tabs"
    ],
    
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
}
